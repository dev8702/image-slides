{
  "name": "react-imageslides",
  "version": "1.2.2",
  "description": "A mobile friendly images slideshow react component",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run clean && node ./scripts/dev.js",
    "build": "npm run clean && webpack --mode production",
    "lint": "eslint --fix ./src --ext .js,.jsx",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react",
    "slide",
    "lightbox"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tests/__mocks__/styleMock.js"
    }
  },
  "author": "loadingwyn",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.52",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.52",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-react": "^7.0.0-beta.52",
    "@storybook/addon-viewport": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "alloyfinger": "^0.1.15",
    "autoprefixer": "^8.6.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.4.0",
    "babel-loader": "^8.0.0-beta.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.3.0",
    "postcss-loader": "^2.1.5",
    "precss": "^3.1.2",
    "prettier": "^1.13.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "superstatic": "^5.0.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "peerDependencies": {
    "alloyfinger": "^0.1.15",
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-dom": "^16.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "css3transform": "^1.1.9",
    "lodash.throttle": "^4.1.1"
  }
}
